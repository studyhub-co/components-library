{
  "name": "@studyhub.co/eval",
  "version": "0.5.2",
  "author": "Studyhub (https://studyhub.co)",
  "license": "MIT",
  "private": false,
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/physics-is-beautiful/pib-eval-library.git"
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "scripts": {
    "start": "node scripts/start.js",
    "install-peers": "install-peers -f",
    "build": "yarn build:clean && yarn build:types && yarn build:lib",
    "build:lib": "cross-env NODE_ENV=production babel src --out-dir lib --extensions \".tsx,.ts\" --copy-files",
    "build:devlib": "cross-env NODE_ENV=production babel src --out-dir lib --extensions \".tsx,.ts\" --copy-files --source-maps inline",
    "build:types": "tsc --emitDeclarationOnly",
    "build:clean": "rimraf lib/*",
    "analyze": "node scripts/build.js && source-map-explorer 'build/static/js/*.js'",
    "test": "CI=true node scripts/test.js",
    "test:watch": "node scripts/test.js --watch",
    "test:coverage": "CI=true node scripts/test.js --env=jsdom --coverage --watchAll=false --coverageReporters=json --coverageReporters=lcov --coverageReporters=text --coverageReporters=html",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --watch --noEmit",
    "lint": "eslint '**/*.@(ts|js|tsx|jsx)'",
    "lint:fix": "eslint '**/*.@(ts|js|tsx|jsx)' --fix"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.0",
    "ace-builds": "^1.4.12",
    "axios": "^0.19.2",
    "babel-preset-react-app": "^9.1.1",
    "clsx": "^1.1.0",
    "evaluatex": "^2.2.0",
    "fabric": "^3.6.3",
    "fp-ts": "^2.6.5",
    "immer": "^6.0.3",
    "io-ts": "^2.2.4",
    "jquery": "^3.5.1",
    "js-quantities": "^1.7.5",
    "mathjs": "^7.3.0",
    "react": "^16.12.0",
    "react-ace": "^9.2.1",
    "react-data-table-component": "^6.11.6",
    "react-dom": "^16.12.0",
    "react-icons": "^3.9.0",
    "react-mathquill": "^0.2.8",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@material-ui/core": "^4.11.0",
    "@svgr/webpack": "4.3.3",
    "@types/fabric": "^3.6.2",
    "@types/jest": "^25",
    "@types/jquery": "^3.5.1",
    "@types/js-quantities": "^1.6.2",
    "@types/node": "^12",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "babel-preset-react-app": "^9.1.1",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "cross-env": "^7.0.0",
    "css-loader": "3.4.2",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "4.3.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "identity-obj-proxy": "3.0.0",
    "install-peers-cli": "^2.2.0",
    "jest": "^25",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "^25",
    "jest-watch-typeahead": "0.4.2",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.6.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^10.2.0",
    "react-dom": "^16.12.0",
    "react-draggable": "^4.4.3",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "resolve": "1.15.0",
    "resolve-url-loader": "3.1.1",
    "rimraf": "^3.0.2",
    "sass": "^1.25.0",
    "sass-loader": "8.0.2",
    "semver": "6.3.0",
    "source-map-explorer": "^2.2.2",
    "style-loader": "0.23.1",
    "styled-components": "^5.0.1",
    "terser-webpack-plugin": "2.3.4",
    "ts-jest": "^25",
    "ts-pnp": "1.1.5",
    "tslib": "^1.10.0",
    "typescript": "^3.7",
    "url-loader": "2.3.0",
    "webpack": "4.41.5",
    "webpack-dev-server": "3.10.2",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "4.3.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
